{
    "name": "enygma/slim-app-skeleton",
    "description": "A skeleton of a Slim application (with extras)",
    "keywords": ["slim", "framework", "skeleton", "slimframework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Cornutt",
            "email": "ccornutt@phpdeveloper.org"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:psecio/validation.git"
        }
    ],
    "require": {
        "slim/slim": "^3.5",
        "slim/twig-view": "^2.1",
        "vlucas/phpdotenv": "^2.4",
        "aura/session": "^2.1",
        "illuminate/database": "^5.3",
        "psecio/validation": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "suggest": {
        "deployer/deployer": "Allows for easier, repeatable application deployments"
    }
}
